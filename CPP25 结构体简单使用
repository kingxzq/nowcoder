#include <iostream>
#include <string>
using namespace std;

struct student {
    // write your code here......
    string name;
    int age;
    double height;
};

int main() {

    // write your code here......
    student st;
    cin>>st.name>>st.age>>st.height;
    cout<<st.name<<" "<<st.age<<" "<<st.height<<endl;

    return 0;
}
