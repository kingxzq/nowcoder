#include <stdio.h>

int main() {
    float a;
    char b;
    scanf("%f %c", &a, &b);
    if(b=='y')
    {
        if(a<=1)
                printf("25");
        else
        {
                if(a<=(int)a)
                        printf("%d",25+((int)a-1));
                else
                        printf("%d",26+((int)a-1));
        }
    }
    if(b=='n')
    {
        if(a<=1)
                printf("20");
        else
        {
                if(a<=(int)a)
                        printf("%d",20+((int)a-1));
                else
                        printf("%d",21+((int)a-1));
        }
    }
    return 0;
}
