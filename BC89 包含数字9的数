#include <stdio.h>

int main() {
    int i=0;
    int j=0;
    int n=0;
    for(i=9;i<=2019;++i)
    {
        j=i;
        while(j>8)
        {
            if(j%10==9)
            {
                n++;
                break;
            }    
            else
                j/=10;
        }
    }
    printf("%d",n);
    return 0;
}
