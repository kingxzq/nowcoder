#include <iostream>
using namespace std;

int main() {

    int arr[6] = { 0 };
    int* ptr = arr;

    int len = sizeof(arr) / sizeof(int);

    for (int i = 0; i < len; i++) {
        cin >> arr[i];
    }

    for (ptr ; ptr < arr + len ; ptr++)
    {
        cout <<  *ptr<< " ";
    }
    

    return 0;
}
