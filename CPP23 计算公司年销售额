#include <iostream>
using namespace std;

int main() {
    // 创建二维数组存储月份销售额
    int money[4][3]={
        {22,66,44},
        {77,33,88},
        {25,45,65},
        {11,66,99}
    };
    //创建一个变量存储年终总额
    int sum = 0;
    // 开始使用循环遍历计算总值
    // 开始逐行相加
    for(int i=0;i<4;i++){
        for(int j=0;j<3;j++){
            sum+=money[i][j];
        }
    }
    // 输出总额
    cout<<sum;
}
