#include<stdio.h>
int main()
{
    int n = 0;
    scanf("%d",&n);
    int arr[20] = {0};
    double sum = 0;
    for(int i = 0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i = 0;i<n;i++)
    {
        if(arr[i]<=60)
        {
            sum += 0.1;
        }
        else 
        {
            sum +=0.2;
        }
    }
    printf("%.1lf\n",sum);
    return 0;
}
