#include <iostream>
using namespace std;
int main() {
    int list[6];
    for (int i = 0; i < 6; i++)
        cin >> list[i];
    cout << "[";
    for (int i = 0; i < 6; i++)
    {
       if(i != 5)
           cout << list[i] << "," << " ";
       else
           cout << list[i];
    }
    cout << "]"<<endl;
    cout << "[";
    for (int i = 5; i >= 0; i--)
    {
        if (i != 0)
            cout << list[i] << "," << " ";
        else
            cout << list[i];
    }
    cout << "]" << endl;
}
