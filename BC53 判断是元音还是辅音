#include <stdio.h>

int main() {
    int a;
    while (scanf("%c", &a) != EOF) {
        getchar();
        if(a=='A'||a=='E'||a=='I'||a=='O'||a=='U'||a=='a'||a=='e'||a=='i'||a=='o'||a=='u')
                printf("Vowel\n");
        else
                printf("Consonant\n");
    }
    return 0;
}
